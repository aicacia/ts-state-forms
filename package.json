{
  "name": "@aicacia/state-forms",
  "version": "0.1.13",
  "description": "use forms with @aicacia/state and @aicacia/state-react",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/aicacia/libs/ts-state-forms.git"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "ts-node node_modules/.bin/tape src/**/*.test.{ts,tsx} | tap-spec",
    "coverage": "nyc --reporter=lcov --require ts-node/register tape src/**/*.test.{ts,tsx} | tap-spec; nyc report ---reporter=text",
    "clean": "rm -rf ./lib",
    "formatTS": "prettier-tslint fix '**/*.{ts,tsx}'",
    "formatJS": "prettier --write '**/*.{js,jsx,json}' --ignore-path .gitignore",
    "format": "npm run formatTS && npm run formatJS",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}' -e 'src/**/*.test.*'"
  },
  "keywords": [
    "state",
    "react",
    "forms"
  ],
  "author": "Nathan Faucett",
  "license": "(MIT OR Apache-2.0)",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@aicacia/changeset": "^0.1.3",
    "@aicacia/state": "^0.1.1",
    "@aicacia/state-react": "^0.1.2",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jsdom": "^16.2.0",
    "@types/react": "^16.9.27",
    "@types/tape": "^4.2.34",
    "@types/uuid": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "immutable": "^4.0.0-rc.12",
    "jsdom": "^16.2.2",
    "nyc": "^15.0.0",
    "prettier": "^2.0.2",
    "prettier-tslint": "^0.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tap-nyc": "^1.0.3",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.2",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@aicacia/debounce": "^0.1.1",
    "@aicacia/json": "^0.1.1",
    "uuid": "^7.0.2"
  },
  "peerDependencies": {
    "immutable": "^4.0.0-rc.12",
    "@aicacia/changeset": "^0.1.3",
    "@aicacia/state": "^0.1.1",
    "@aicacia/state-react": "^0.1.1",
    "react": "^16.13.1"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "src/**/*.test.*"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ]
  }
}
