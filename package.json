{
  "name": "@stembord/state-forms",
  "version": "0.2.12",
  "description": "use forms with @stembord/state and @stembord/state-react",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/stembord/libs/ts-state-forms.git"
  },
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "module": "./lib/index.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "ts-node ./node_modules/tape/bin/tape ./tests/**/*.test.{ts,tsx} | tap-spec",
    "clean": "rm -rf ./src",
    "format.ts": "prettier-tslint fix '**/*.{ts,tsx}'",
    "format.js": "prettier --write '**/*.{js,jsx,json}' --ignore-path .gitignore",
    "format": "npm run format.ts && npm run format.js",
    "lint": "tslint -c tslint.json 'lib/**/*.{ts,tsx}'"
  },
  "keywords": [
    "state",
    "react",
    "forms"
  ],
  "author": "Nathan Faucett",
  "license": "(MIT OR Apache-2.0)",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@stembord/changeset": "^0.1.5",
    "@stembord/state": "^0.2.5",
    "@stembord/state-react": "^0.2.10",
    "@types/enzyme": "^3.9.0",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jsdom": "^12.2.2",
    "@types/react": "^16.8.4",
    "@types/tape": "^4.2.33",
    "@types/uuid": "^3.4.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "immutable": ">= 4.0.0-rc.12",
    "jsdom": "^13.2.0",
    "prettier": "^1.16.4",
    "prettier-tslint": "^0.4.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "tap-spec": "^5.0.0",
    "tape": "^4.10.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4"
  },
  "dependencies": {
    "@stembord/debounce": "^0.1.3",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "immutable": ">= 4.0.0-rc.12",
    "@stembord/changeset": "^0.1.5",
    "@stembord/state": "^0.2.5",
    "@stembord/state-react": "^0.2.10",
    "react": ">= 16.8.6"
  }
}
